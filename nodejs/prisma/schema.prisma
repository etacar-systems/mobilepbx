generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id  Int @id @default(autoincrement())
  name String
  email String
  age Int
  createdAt DateTime @default(now())
}

model Dispatcher {
  id  Int @id @default(autoincrement())
  setid Int 
  destination String
  socket String
  state Int
  weight Int
  priority Int
  attrs Int
  description String
}